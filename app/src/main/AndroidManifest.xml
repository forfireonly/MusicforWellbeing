<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.annascott.musicforwellbeing">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RunningMusicActivity"
            android:label="Motivational Music for Running"/>
        <activity android:name=".WeightLiftingMusicActivity"
            android:label="Best Pump Up Songs for Gym"/>
        <activity android:name=".StretchingMusicActivity"
            android:label="Meditation Music for Stretching"/>
        <activity android:name=".NatureMusicActivity"
            android:label="Nature Sounds for Relaxation"></activity>
    </application>

</manifest>